{
  "name": "robinhq/connect-server",
  "description": "Example integration with robinhq/connect based on laravel/lumen",
  "keywords": [
    "framework",
    "robinhq",
    "connect",
    "example",
    "intergration"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "laravel/lumen-framework": "5.1.*",
    "vlucas/phpdotenv": "~1.0",
    "robinhq/connect": "~0.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "fzaninotto/faker": "~1.0",
    "laravel/homestead": "v2.1.2"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "database/"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
